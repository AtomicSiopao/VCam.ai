{
  "name": "thelab",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "cypress": "15.6.0",
    "cypress-real-events": "^1.15.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2"
  },
  "scripts": {
    "cy:open": "cypress open",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:electron": "cypress run --browser electron",
    "e2e:headed": "cypress run --headed",
    "test": "cypress run && pnpm run report",
    "merge-reports": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/report.json",
    "generate-report": "marge cypress/reports/report.json -f final-report -o cypress/reports/html",
    "report": "pnpm run merge-reports && pnpm run generate-report"
  }
}
