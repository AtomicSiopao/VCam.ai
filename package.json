{
  "name": "vcamai",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
    "cypress": "15.6.0",
    "cypress-real-events": "^1.15.0",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2"
  },
  "scripts": {
    "cy:open": "cypress open",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:electron": "cypress run --browser electron",
    "e2e:headed": "cypress run --headed",
    "test": "cypress run && pnpm run report",
    "merge-reports": "pnpm mochawesome-merge cypress/reports/*-report.json > cypress/reports/report.json",
    "generate-report": "pnpm marge cypress/reports/report.json --reportDir cypress/reports --inline",
    "report": "pnpm run merge-reports && pnpm run generate-report"
  }
}
